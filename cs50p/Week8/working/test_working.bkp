import pytest
from working import convert

def test_convert_invalid_from_time():
    with pytest.raises(ValueError):
        convert("13:00 AM to 9:00 PM")
    with pytest.raises(ValueError):
        convert("a:00 AM to 9:00 PM")
    with pytest.raises(ValueError):
        convert("05:60 AM to 9:00 PM")
    with pytest.raises(ValueError):
        convert("05:30 to 9:00 PM")


def test_convert_invalid_to_time():
    with pytest.raises(ValueError):
        convert("05:00 AM to 25:00 PM")
    with pytest.raises(ValueError):
        convert("5:00 AM to a:00 PM")
    with pytest.raises(ValueError):
        convert("05:00 AM to 9:60 PM")
    with pytest.raises(ValueError):
        convert("05:30 to 9:00")


def test_convert_invalid_format():
    with pytest.raises(ValueError):
        convert("05:00 AM 9:00 PM")
    with pytest.raises(ValueError):
        convert("5 AM to 9:00 PM")
    with pytest.raises(ValueError):
        convert("05:00 AM to 9 PM")
    with pytest.raises(ValueError):
        convert("9 AM - 5 PM")
    with pytest.raises(ValueError):
        convert("09:00 AM - 17:00 PM")


def test_convert_valid():
    assert convert("9 AM to 5 PM") == "09:00 to 17:00"
    assert convert("9:00 AM to 5:00 PM") == "09:00 to 17:00"
    assert convert("10 PM to 8 AM") == "22:00 to 08:00"
    assert convert("10:30 PM to 8:50 AM") == "22:30 to 08:50"
    assert convert("00:00 AM to 12:00 PM") == "00:00 to 12:00"
    assert convert("0 AM to 12 PM") == "00:00 to 12:00"
    assert convert("12:00 AM to 12:00 PM") == "00:00 to 12:00"
    assert convert("12 AM to 12 PM") == "00:00 to 12:00"